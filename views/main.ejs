<%- include("head") %>
<body>
    <!-- เป็นหน้าหลักในการใช้งานของแต่ละ user (ยังไม่เสร็จ) -->
    <table class="table table-striped table-bordered">
        <thead>
            <tr>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Phone</th>
                <th>Email</th>
                <th>Address</th>
                <th>Date of Birth</th>
                <th>Gender</th>
            </tr>
        </thead>
        <tbody>
            <% data.forEach((item) => { %>
                <tr>
                    <td><%= item.first_name %></td>
                    <td><%= item.last_name %></td>
                    <td><%= item.phone %></td>
                    <td><%= item.email %></td>
                    <td><%= item.address %></td>
                    <td><%= item.dob %></td>
                    <td><%= item.gender %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>